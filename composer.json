{
  "name": "ajur-media/steamboat.engine",
  "description": "Steamboat Engine",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Karel Wintersky",
      "email": "karel.wintersky@gmail.com"
    }
  ],
  "config": {
    "optimize-autoloader": true
  },
  "require": {
    "php": ">=7.2",
    "ext-curl": "*",
    "ext-gd": "*",
    "ext-iconv": "*",
    "ext-intl": "*",
    "ext-json": "*",
    "ext-mbstring": "*",
    "ext-mysqli": "*",
    "ext-pdo": "*",
    "ext-simplexml": "*",
    "psr/log": "^1.1"
  },
  "require-dev": {
    "smarty/smarty": "^3.1"
  },
  "autoload": {
    "files": [
      "functions/functions.php"
    ],
    "psr-4": {
      "SteamBoat\\"           : ["sources/", "interfaces/"]
    }
  }
}
